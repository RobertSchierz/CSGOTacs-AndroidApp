<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"
        android:color="@color/my_profile_background" /> <!-- pressed -->
    <item android:state_focused="true"
        android:color="@color/my_profile_background" /> <!-- focused -->
    <item android:state_selected="true"
        android:color="@color/my_profile_background" /> <!-- selected -->
    <item android:state_checked="true"
        android:color="@color/my_profile_background" /> <!-- checked -->
    <item android:state_activated="true"
        android:color="@color/my_profile_background" /> <!-- activated -->
    <item android:color="@color/title_text" /> <!-- default -->
</selector>